<% if(success && success.length){ %>
<div class="alert alert-success alert-dismissible  offset-1 col-10" role="alert" style="z-index: 100;">
    <%= success %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>

<% if(error && error.length){ %>
    <div class="alert alert-danger alert-dismissible  offset-1 col-10" role="alert" role="alert" style="z-index: 100;">
        <%= error %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    <% } %>
